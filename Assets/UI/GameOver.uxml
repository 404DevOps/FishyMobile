            <ui:Label text="Score: 123" display-tooltip-when-elided="true" name="txtScore" style="color: rgb(229, 97, 0); font-size: 42px; -unity-text-align: upper-center; margin-left: 10px; margin-right: 10px; margin-top: 10px; margin-bottom: 10px; -unity-font-style: bold;" />
